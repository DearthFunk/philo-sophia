{
  "name": "philosobabel",
  "version": "1.0.0",
  "description": "Bunch of short definitions used as a quick reference guide for stupid philosophy discussions online. Words and their definitions are stupid.",
  "license": "ISC",
  "author": "dearth funk",
  "type": "module",
  "main": "src/index.tsx",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "dev": "webpack --mode development --watch",
    "dev:serve": "webpack serve --mode development --open",
    "type-check": "tsc --project tsconfig.check.json",
    "type-check:watch": "tsc --project tsconfig.check.json --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DearthFunk/philosobabel.git"
  },
  "keywords": [
    "philosophy"
  ],
  "bugs": {
    "url": "https://github.com/DearthFunk/philosobabel/issues"
  },
  "homepage": "https://github.com/DearthFunk/philosobabel#readme",
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.32.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.32.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
